default: &default
  adapter: mysql2
  encoding: utf8
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  username: root
  password:
  socket: /tmp/mysql.sock

development:
  <<: *default
  database: henslowe5_development

test:
  <<: *default
  database: henslowe5_test
  host: 127.0.0.1

production:
  <<: *default
  database: henslowe5_production
